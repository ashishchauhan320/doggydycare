.staff_section {
    padding: 85px 0 0;
}

.staff_section h2 {
    color: #000000;
    font-family: "proxima_nova_rgbold";
    font-size: 44px;
    margin-bottom: 45px;
    text-transform: uppercase;
}

.staff_box {
    margin-bottom: 40px;
    min-height: 550px;
}

.staff_section h4 {
    color: #000;
    font-size: 22px;
