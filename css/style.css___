/**********

select::-ms-expand {
 display: none;
}

select { background-image: url(../images/select-dd.png); background-position: right; background-repeat: no-repeat; -webkit-appearance: none; -moz-appearance: none; -o-appearance: none; appearance: none; }
*/

::selection {
    background: rgba(14, 120, 174, 0.6);
    color: #fff;
}

::-moz-selection {
    background: rgba(14, 120, 174, 0.6);
    color: #fff;
}

a {
    text-decoration: none !important;
    -webkit-transition: all 0.3s ease-in-out;
    -moz-transition: all 0.3s ease-in-out;
    -ms-transition: all 0.3s ease-in-out;
    transition: all 0.3s ease-in-out;
}

*:focus {
    outline: none !important
}

img {
    max-width: 100%
}

strong,
b {
    font-weight: 700;
}

i,
em {
    font-style: italic;
}

.clr {
    border: 0;
    clear: both;
    height: 0;
}

html,
body {
    width: 100%;
    height: 100%;
}

h1,
h2,
h3,
h4,
h5,
h6,
p {
    font-weight: normal;
    margin: 0;
}

body {
    background: #fff;
}

.navbar-default {
    background: none;
    border: 0 none;
}

.carousel {
    transition: all 0.5s ease;
    -webkit-transition: all 0.5s ease;
    -moz-transition: all 0.5s ease;
    -ms-transition: all 0.5s ease;
}

.banner img {
    width: 100%;
}

.main_menu {
    margin: 12px 0 35px;
}

.main_menu ul {
    margin-top: 36px;
}

.main_menu ul li {
    margin-left: 55px;
}

.main_menu ul li a {
    font-size: 18px;
    text-transform: uppercase;
    font-family: 'proxima_nova_rgbold';
    padding: 0;
    color: #202020 !important;
}

.main_menu ul li a:hover {
    color: #ff0000 !important;
}

.main_menu ul li:last-child {
    border-left: 1px solid #202020;
    margin-left: 20px;
    margin-top: -5px;
    padding-left: 20px;
}

.main_menu .navbar-nav > .active > a,
.main_menu .navbar-nav > a:hover,
.main_menu .navbar-nav > .active > a:focus {
    background: none;
    color: #ff0000 !important;
}

.head-section-bg {
    background: #313131;
    padding: 10px 0;
}

.contact-dtl {
    padding: 0;
}

.contact-dtl li {
    float: left;
    list-style: none;
    font-size: 18px;
    margin-right: 25px;
}

.contact-dtl li a {
    color: #fff;
}

.contact-dtl li a:hover {
    color: #ff0000;
}

.contact-dtl li a i {
    margin-right: 12px;
    color: #868686;
}

.social_icon li {
    list-style: none;
    float: left;
    margin-left: 18px;
}

.social_icon li i {
    color: #313131;
    font-size: 18px;
    text-shadow: 1px 2px 0 #818181;
    transition: all 0.3s ease;
    -webkit-transition: all 0.3s ease;
    -moz-transition: all 0.3s ease;
    -ms-transition: all 0.3s ease;
}

.social_icon li i:hover {
    color: #fff;
}

.navbar-collapse {
    padding: 0;
}

.navbar-brand {
    padding: 0;
    height: 0;
}

.get_in_touch {
    font-size: 48px;
    color: #010101;
    font-family: 'EducationPencil';
    background: #fbf318;
    width: 646px;
    height: 74px;
    line-height: 74px;
    text-align: center;
    position: absolute;
    top: 55%;
    left: 12%;
    border-radius: 30px;
    margin-top: 25px;
    transition: all 0.5s ease;
    -webkit-transition: all 0.5s ease;
    -moz-transition: all 0.5s ease;
    -ms-transition: all 0.5s ease;
    cursor: pointer;
}

.get_in_touch:hover {
    color: #fff;
    background: #ff0000;
}

.service_section {
    background-attachment: scroll;
    background-clip: border-box;
    background-color: #ffffff;
    background-image: url("../images/service_bg.jpg");
    background-origin: padding-box;
    background-position: 100% 0;
    background-repeat: no-repeat;
    height: auto;
    padding: 170px 0 0;
    position: relative;
    width: 100%;
}

.dog_coller {
    left: 192px;
    position: absolute;
    top: -95px;
}

.service_section h2 {
    text-align: center;
    font-size: 68px;
    font-family: 'EducationPencil';
    margin-bottom: 90px;
    color: #3c3c3c;
    text-transform: capitalize;
    letter-spacing: 2px;
}

.service_section h3 {
    font-family: 'ProximaNova-Light';
    font-size: 48px;
    position: relative;
    color: #3c3c3c;
    margin-bottom: 18px;
    z-index: 999;
    padding: 75px 0 0 70px;
}

.service_section h3:before {
    content: "";
    width: 148px;
    height: 148px;
    border-radius: 50%;
    position: absolute;
    top: 0;
    left: 0;
    background: #eaeaea;
    z-index: -1;
}

.service_section p {
    font-family: 'ProximaNova-Regular';
    font-size: 16px;
    color: #3c3c3c;
    line-height: 30px;
    margin: 36px 0 0 128px;
}

.service_section a {
    border-bottom: 1px solid #3c3c3c;
    color: #3c3c3c;
    display: inline-block;
    font-family: "ProximaNova-Light";
    font-size: 18px;
    margin: 46px 0 0 128px;
    padding-bottom: 5px;
    transition: all 0.3s ease;
    -webkit-transition: all 0.3s ease;
    -moz-transition: all 0.3s ease;
    -ms-transition: all 0.3s ease;
}

.service_section a:hover {
    color: #ff0000;
    border-bottom: 1px solid #ff0000;
}

.box {
    margin-bottom: 110px;
}

.testimonial_section {
    height: 100%;
    position: relative;
    overflow: hidden;
}

.testimonial_section .bnr {
    width: 100%;
}

.coma {
    width: auto;
}

#testim-slider {
    margin-top: 5px;
}

.testim_box {
    position: absolute;
    right: 315px;
    top: 208px;
    width: 910px;
    text-align: center;
    overflow: hidden;
    z-index: 10;
}

.testimonial_section h3 {
    font-size: 48px;
    font-family: 'ProximaNova-Regular';
    color: #595757;
    margin: 0 0 40px 0;
}

#testim-slider .item {
    float: left;
    text-align: center;
    color: #212121;
    font-size: 18px;
    margin: 25px 0 0;
}

.owl-carousel .owl-item {
    float: left;
    overflow: hidden;
}

#brand-slider {
    overflow: hidden;
}

#testim-slider h4 {
    color: #ed1c24;
    font-size: 25px;
    font-family: 'ProximaNova-Regular';
    margin: 30px 0;
}

#brand-slider .item {
    margin: 0 60px;
}

#brand-slider .owl-buttons {}

#brand-slider .owl-buttons .owl-prev {
    left: 0;
    position: absolute;
    top: 12px;
    cursor: pointer;
    background: #dbdbdb none repeat scroll 0 0;
    color: #ffffff;
    padding: 6px 9px 7px;
    border-radius: 0;
    transition: all 0.3s ease;
    -webkit-transition: all 0.3s ease;
    -moz-transition: all 0.3s ease;
    -ms-transition: all 0.3s ease;
}

#brand-slider .owl-buttons .owl-prev:hover {
    background: #ff0000;
}

#brand-slider .owl-buttons .owl-prev i {
    font-size: 35px;
}

#brand-slider .owl-buttons .owl-next i {
    font-size: 35px;
}

#brand-slider .owl-buttons .owl-next {
    position: absolute;
    border-radius: 0;
    right: 0;
    top: 12px;
    cursor: pointer;
    background: #dbdbdb none repeat scroll 0 0;
    color: #ffffff;
    padding: 6px 9px 7px;
    transition: all 0.3s ease;
    -webkit-transition: all 0.3s ease;
    -moz-transition: all 0.3s ease;
    -ms-transition: all 0.3s ease;
}

#brand-slider .owl-buttons .owl-next:hover {
    background: #ff0000;
}

.footer {
