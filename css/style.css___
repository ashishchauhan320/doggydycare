}

.founder_Section {
    background: #f2f2f2;
    padding: 50px 0;
}

.founder_Section h2 {
    font-size: 44px;
    color: #000;
    font-family: 'proxima_nova_rgbold';
    text-transform: uppercase;
    margin-bottom: 45px;
}

.founder_Section h3 {
    font-size: 38px;
    color: #252525;
    font-family: 'proxima_nova_rgbold';
    text-transform: capitalize;
    margin-bottom: 5px;
}

.founder_Section h3 small {
    color: #646464;
    font-size: 30px;
}

.founder_Section p {
    font-size: 18px;
