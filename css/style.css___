}

.staff_section h5 {
    color: #424040;
    font-size: 16px;
    font-family: 'ProximaNova-Regular';
    margin-bottom: 20px;
}

.staff_section p {
    color: #000;
    font-size: 18px;
    font-family: 'ProximaNova-Regular';
    line-height: 30px;
}

.brands_section h2 {
    text-align: center;
    font-size: 48px;
    color: #424242;
    font-family: 'ProximaNova-Regular';
    margin-bottom: 60px;
}

.owl-theme .owl-controls {
    margin-top: 10px;
    text-align: center;
}

.owl-theme .owl-controls .owl-buttons div {
    background: #869791 none repeat scroll 0 0;
    border-radius: 30px;
    color: #ffffff;
    display: inline-block;
    font-size: 12px;
    margin: 5px;
    opacity: 0.5;
    padding: 3px 10px;
}

.owl-theme .owl-controls.clickable .owl-buttons div:hover {
    opacity: 1;
    text-decoration: none;
}

.owl-theme .owl-controls .owl-page {
    display: inline-block;
}

.owl-theme .owl-controls .owl-page span {
    background: #ffffff none repeat scroll 0 0;
    border-radius: 0;
    display: block;
    height: 6px;
    margin: 5px 7px;
    opacity: 0.5;
    width: 50px;
}

.owl-theme .owl-controls .owl-page.active span,
.owl-theme .owl-controls.clickable .owl-page:hover span {
    opacity: 1;
    background: #ff0000;
}

.owl-theme .owl-controls .owl-page span.owl-numbers {
    border-radius: 30px;
    color: #ffffff;
    font-size: 12px;
    height: auto;
    padding: 2px 10px;
    width: auto;
}

.owl-item.loading {
    background: rgba(0, 0, 0, 0) url("AjaxLoader.gif") no-repeat scroll center center;
    min-height: 150px;
}

#brand-slider .owl-pagination {
    display: none !important;
}

.carousel-indicators li {
    background-color: #fff;
    border: 2px solid #ffffff;
    border-radius: 10px;
    cursor: pointer;
    display: inline-block;
    height: 10px;
    margin: 1px;
    text-indent: -999px;
    width: 10px;
}

.carousel-indicators .active {
    background-color: rgba(0, 0, 0, 0);
    height: 12px;
    margin: 0;
    width: 12px;
}

.map {
    margin-top: 95px;
    position: relative;
}

.map iframe {
    width: 100%;
    height: 570px;
}

.box-toggle {}

.address_box {
    background: #232323 none repeat scroll 0 0;
    height: 270px;
    left: 0;
    position: absolute;
    top: 27%;
    width: 584px;
    text-align: center;
    overflow: hidden;
    transition: all 0.6s ease;
    -webkit-transition: all 0.6s ease;
    -moz-transition: all 0.6s ease;
    -ms-transition: all 0.6s ease;
}

.address_box.highlight {
    left: -562px;
}

.box_inner {
    height: 152px;
    margin-top: -76px;
    position: relative;
    top: 50%;
}

.address_box h2 {
    color: #b2b2b2;
    font-size: 22px;
    font-family: 'ProximaNova-Regular';
}

.address_box p {
    color: #b2b2b2;
    font-size: 18px;
    font-family: 'ProximaNova-Regular';
    line-height: 23px;
}

.address_box a.phn {
    margin-top: 40px;
}

.address_box a {
    color: #b2b2b2;
    display: inline-block;
    font-family: "ProximaNova-Regular";
    font-size: 18px;
}

.address_box a:hover {
    color: #fff;
}

.click_tg {
    background: #353535 none repeat scroll 0 0;
    height: 270px;
    left: 561px;
    position: absolute;
    text-align: center;
    top: 0px;
    width: 22px;
    cursor: pointer;
    transition: 0.38s ease-in;
    -webkit-transition: 0.38s ease-in;
    -moz-transition: 0.38s ease-in;
    -ms-transition: 0.38s ease-in;
}

.click_tg.add_transition {
    left: 0;
}

.click_tg.alt {
    background: #353535 none repeat scroll 0 0;
    height: 270px;
    left: 561px;
    position: absolute;
    text-align: center;
    top: 53%;
    transform: translateY(-51%);
    -webkit-transform: translateY(-51%);
    -moz-transform: translateY(-51%);
    -ms-transform: translateY(-51%);
    width: 22px;
}

.click_tg > img {
    height: 6px;
    margin-top: -3px;
    position: relative;
    text-align: center;
    top: 50%;
}

.click_tg.alt > img {
    height: 6px;
    margin-top: -3px;
    position: relative;
    text-align: center;
    top: 50%;
}

@media (min-width: 1500px) {
    .container {
        width: 1470px;
    }
}

@media (max-width: 1499px) {
    .main_menu ul li {
        margin-left: 35px;
    }
    .main_menu ul li a {
        font-size: 15px;
    }
    .dog_coller {
        left: 30px;
    }
    .testim_box {
        right: 42px;
        top: 108px;
    }
    .get_in_touch {
        width: 460px;
    }
    .care_section h2 {
        font-size: 32px;
    }
    .staff_box {
        margin-bottom: 40px;
        min-height: 525px;
    }
    .testimonial_section h3 {
        font-size: 48px;
        font-family: 'ProximaNova-Regular';
        color: #595757;
        margin: 0 0 30px 0;
    }
    #testim-slider {
        margin-top: 0;
    }
    .testimonial_section h3 {
        margin: 0 0 15px 0;
    }
}